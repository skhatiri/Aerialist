# the root folder for PX4-Autopilot
# Full address, do not use '~/'
PX4_HOME=/src/PX4-Autopilot/
CATKIN_HOME=/src/catkin_ws/
# run >> roslaunch-logs to find out the correct address
ROS_HOME=/root/.ros/
# where to copy the generated logs to (will not copy if not present)
LOGS_COPY_DIR=results/
# A folder to save the experiment results
RESULTS_DIR=results/
THRESHOLD_FILE=threshold.csv
THRESHOLD_FILE_EDIT_MODE=a
DATASET_FILE=dataset_file_combined
DATA_FILE_EDIT_MODE=a
CPU_FILE=cpu_file
CPU_FILE_EDIT_MODE=a

# test environment defaults, you probably do not need to change them
SIMULATOR=ros
SPEED=1
DRONE=ros
HEADLESS=False
AVOIDANCE_WORLD=simple_obstacle
WAITING_TIME=10
#AVOIDANCE_LAUNCH=aerialist/resources/simulation/local_planner_stereo.launch
AVOIDANCE_LAUNCH=aerialist/resources/simulation/collision_prevention.launch
AVOIDANCE_BOX=/src/aerialist/aerialist/resources/simulation/box.xacro
# PARAMS=resources/simulation/params_avoidance.csv

# Positioning system
USE_GPS=False
PLOT_TESTS_XYZ=False
ALLIGN_ORIGIN=True
# RESAMPLE=True
# TRJ_SMPL_PRD=500000
# DISTANCE_METHOD=frechet
# DISTANCE_METHOD=dtw-t
# DISTANCE_METHOD=dtw
# AVE_GAMMA=3
# AVERAGE_METHOD=dtw

# DOCKER
DOCKER_IMG=skhatiri/aerialist
DOCKER_TIMEOUT=1000
# SIMULATION_TIMEOUT=970

# k8s
ROS_KUBE_TEMPLATE=aerialist/resources/k8s/k8s-job-avoidance-pattern.yaml
# KUBE_TEMPLATE=aerialist/resources/k8s/k8s-job.yaml

# webdav server
# WEBDAV_DL_FLD=/tmp/
# WEBDAV_UP_FLD=tmp/
WEBDAV_UP_FLD=webdav://test/

## Data for image folder
# UPLOAD_LIST=/src/histogram_image,/src/disparity_image,/src/raw_image
UPLOAD_LIST=

